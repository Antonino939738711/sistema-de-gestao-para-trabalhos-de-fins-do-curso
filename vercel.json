{
  "version": 2,
  "builds": [
    {
      "src": "SGTFC/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "SGTFC/wsgi.py"
    }
  ],
  "env": {},
  "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate"
}
